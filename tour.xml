<krpano version="1.18" title="Virtual Tour" onstart="startup();">

	<include url="skin/vtourskin.xml" />

	<!-- set skin settings: bingmaps? gyro? thumbnails? tooltips? -->
	<skin_settings 
	               gyro="true"
	               title=""
	               thumbs="true"
	               thumbs_width="120" thumbs_height="80" thumbs_padding="10" thumbs_crop="0|40|240|160"
	               thumbs_opened="false"
	               thumbs_text="false"
	               thumbs_dragging="true"
	               thumbs_onhoverscrolling="false"
	               thumbs_scrollbuttons="false"
	               thumbs_scrollindicator="false"
	               thumbs_loop="false"
	               tooltips_thumbs="false"
	               tooltips_hotspots="false"
	               tooltips_mapspots="false"
	               loadscene_flags="MERGE"
	               loadscene_blend="BLEND(0.5)"
	               controlbar_offset="20"
	               />

	<!-- set optional skin logo url -->
	<layer name="skin_logo" url="" scale="0.25" opened_onclick="openurl('...',_blank);" />


	<action name="startup">
		if(startscene === null, copy(startscene,scene[0].name));
		loadscene(get(startscene), null, MERGE);
	</action>

<events onloadcomplete="js ( DisHtml(get(xml.scene)); ) ; " />

	<!-- events - update logo rotation with pano rotation -->
	<events name="nadirlogorotation"
	        onviewchange="if(hotspot[nadirlogo].rotatewithview, set(hotspot[nadirlogo].rotate,get(view.hlookat)) );"
	        />




	<scene name="entryway" title="Entryway" onstart="" thumburl="panos/node07.tiles/thumb.jpg" lat="" lng="" heading="">

		<view hlookat="0" vlookat="0" fovtype="MFOV" fov="120" maxpixelzoom="2.0" fovmin="70" fovmax="140" limitview="auto" />

		<preview url="panos/node07.tiles/preview.jpg" />

		<image type="CUBE" multires="true" tilesize="512">
			<level tiledimagewidth="1304" tiledimageheight="1304">
				<cube url="panos/node07.tiles/%s/l2/%v/l2_%s_%v_%h.jpg" />
			</level>
			<level tiledimagewidth="652" tiledimageheight="652">
				<cube url="panos/node07.tiles/%s/l1/%v/l1_%s_%v_%h.jpg" />
			</level>
			<mobile>
				<cube url="panos/node07.tiles/mobile_%s.jpg" />
			</mobile>
		</image>

		<!-- place your scene hotspots here -->
	<!-- nadir logo (distored hotspot) -->
	<hotspot name="nadirlogo"
	         url="skin/nadirlogo.png"
	         ath="0"
	         atv="90"
	         distorted="true"
	         scale="1.0"
	         rotate="0.0"
	         rotatewithview="false"
	         onclick="switch(rotatewithview); invalidatescreen();"
	         />

	</scene>

